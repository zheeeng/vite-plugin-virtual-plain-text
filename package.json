{
  "name": "vite-plugin-virtual-plain-text",
  "author": "Zheeeng <hi@zheeeng.me>",
  "description": "A Vite plugin loads file as plain text from the virtual assets workspace.",
  "keywords": [
    "vite",
    "plugin",
    "plain text",
    "virtual workspace",
    "virtual assets"
  ],
  "repository": "https://github.com/zheeeng/vite-plugin-virtual-plain-text",
  "version": "1.4.2",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "dev": "vite playground",
    "prepublishOnly": "npm run build",
    "build": "tsup"
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "devDependencies": {
    "@types/node": "^18.11.18",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vite": "^2.3.4"
  }
}
